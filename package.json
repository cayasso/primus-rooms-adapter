{
  "name": "primus-rooms-adapter",
  "version": "0.3.2",
  "description": "In-memory default adapter for primus-rooms-adapter, use as abstract class",
  "author": "Jonathan Brumley <cayasso@gmail.com>",
  "homepage": "https://github.com/cayasso/primus-rooms-adapter#readme",
  "main": "./lib/index",
  "scripts": {
    "test": "xo ./src/* && mocha",
    "build": "rm -rf lib && mkdir -p lib && async-to-gen -d lib src",
    "prepublish": "npm run build"
  },
  "repository": "/cayasso/primus-rooms-adapter",
  "keywords": [
    "primus",
    "primus-rooms",
    "rooms",
    "store",
    "memory",
    "engine",
    "broadcast",
    "in-memory"
  ],
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "async-to-gen": "1.3.2",
    "should": "~11.2.0",
    "mocha": "~3.2.0",
    "pre-commit": "^1.2.2",
    "xo": "^0.17.1"
  },
  "pre-commit": [
    "test"
  ],
  "xo": {
    "semicolon": false,
    "esnext": true,
    "space": 2,
    "ignores": [
      "test/**"
    ],
    "rules": {
      "curly": 0,
      "object-curly-spacing": 0,
      "no-labels": 0,
      "no-unused-labels": 0,
      "no-unused-expressions": 0,
      "no-implicit-coercion": 0,
      "max-lines": 0,
      "yoda": 0,
      "no-negated-condition": 0
    }
  }
}
